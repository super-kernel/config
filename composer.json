{
    "name": "super-kernel/config",
    "type": "library",
    "license": "MIT",
    "description": "The config component for super-kernel.",
    "keywords": [
        "php",
        "swoole",
        "super-kernel",
        "config"
    ],
    "autoload": {
        "psr-4": {
            "SuperKernel\\Config\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.4",
        "super-kernel/contract": "*"
    },
    "require-dev": {
        "super-kernel/di": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true
    }
}